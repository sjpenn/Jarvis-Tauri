# JARVIS Environment Configuration
# Copy this to .env and fill in your API keys

# Ollama (local, no key needed)
OLLAMA_HOST=http://localhost:11434

# ElevenLabs (optional, for premium TTS)
ELEVENLABS_API_KEY=

# Porcupine Wake Word (optional, free tier available)
PORCUPINE_ACCESS_KEY=

# Perplexity API (optional, for web search)
PERPLEXITY_API_KEY=

# Google APIs (optional, for Gmail/Calendar)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
